@using RazorLight
@using System.Linq
@using ApiGenerator.Domain
@inherits TemplatePage<RestApiSpec>
using System;
using System.Collections.Generic;
using System.Collections.Specialized;
using System.Linq;
using System.Text;
using System.Threading;
using System.Threading.Tasks;
using Elasticsearch.Net;

//This file is automatically generated from https://github.com/elastic/elasticsearch/tree/master/rest-api-spec

namespace Nest
{
	///<summary>
	///Raw operations with elasticsearch
	///</summary>
	public partial interface ITempClient 
	{
		@foreach(var kv in Model.EndpointsPerNamespace)
		{
			var endpoints = kv.Value;
			var methods = endpoints.Select(e=>e.HighLevelClientMethod).ToList();
			foreach(var method in methods)
			{
				await IncludeAsync("HighLevelClient/MethodInterface.cshtml", method);
			}
		}
	}
}
